    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.2.0",
    "moment": "^2.26.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.5.11",
    "react-number-format": "^4.4.1",
